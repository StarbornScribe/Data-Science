from datetime import datetime

formats = [" %A, %B %d, %Y", " %A, %d %B %Y", " %A, %d.%m.%y"]

with open('second_dz/newspapers.txt', encoding = 'utf-8') as file:
    for line in file:
        line = line.strip().split('-')
        
        for i in formats:
            try:
                date = datetime.strptime(line[1], i)
                print(date)
            except:
                pass
